{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "pwa-node",
          "request": "launch",
          "name": "Run Test Debugger",
          "runtimeExecutable": "npm",
          "runtimeArgs": [
            "run",
            "dev"
          ],
          "cwd": "${fileDirname}",
          "skipFiles": [
              "<node_internals>/**/**",
              "node_modules/**/**"
          ],
          "console": "integratedTerminal"
      }
  ],
  "compounds": []
}
